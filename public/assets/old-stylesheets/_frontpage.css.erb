.logo {
  min-height: 87px;
  padding-left: 280px;
  padding-top: 10px;
  background: url(/images/riseup-rainbow.png) no-repeat;
}
@media only screen and (max-width: 590px) {
  .logo {
     padding-left: 94px;
     padding-top: 65px;
  }
}

.title {
  overflow: hidden;
}
<%##
  ## SERVICES
  ##%>

.left_column h2 {
  font-size: 2em;
  color: #333;
  border: 0;
  margin: 20px 0 0 0;
}
.left_column a.anchor {
  display: none;
}

<%##
  ## STATUS FEED
  ##%>

.feed {
  border: 1px dotted #ccc;
  margin-right: -30px;
}

.feed .item {
  border-top: 1px dotted #ccc;
  padding: <%= gutter(6) %>%;
  padding-right: 30px;
}
.feed h1 {
  font-weight: bold;
  font-size: 1.1em;
  line-height: 1.5em;
  padding: <%= gutter(6) %>%;
  padding-top: <%= gutter(6)/2 %>%;
  padding-bottom: <%= gutter(6)/2 %>%;
}
.item {
}
.item h2 {
  font-weight: bold;
  font-size: 1em;
  line-height: 1.5em;
  color: #666;
}
.item .date {
  font-style: italic;
  line-height: 2em;
}
